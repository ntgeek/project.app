.icon{
  width: 100rpx;
  height:90rpx;
}
.tabbar {
  width: 100vw;
  position: fixed;
  display: flex;
  bottom:0;
  padding:10rpx;
  background:rgb(252, 252,252);
  font-size:20rpx;
  height: 9.8vh;
}
.tabbar-item{
  float: flex;
  width: 25%;
  height: auto;
  position: relative;
  left: 100rpx;
  overflow: hidden;
  text-align: center;
  top:30rpx;
}
.tabbar-item:nth-child(2){
  position: relative;
   top: -100rpx;
  height:280rpx;
  flex: 0.7;
}

.tabbar-item:nth-child(2) .tabbar-item-icon image{
  position: absolute;
  top: -1rpx;
  width: 234rpx;
  height: 247.5rpx;
  left: -30rpx;
  z-index: 0;
}

.tabbar-item:nth-child(1) .tabbar-item-icon image{
  position: relative;
  top:-25rpx;
  left: -30rpx;
  width: 120rpx;
  height: 130rpx;
}
.tabbar-item:nth-child(3) .tabbar-item-icon image{
  position: relative;
  top: -15rpx;
  left: -70rpx;
  width: 108rpx;
  height: 108rpx;
}
.active {
  color: #23ac38;
}
